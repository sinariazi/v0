{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run prisma generate && npm run build"
      }
    }
  ],
  "env": {
    "AWS_REGION": "@aws_region",
    "SECRET_NAME": "@secret_name",
    "DATABASE_URL": "@database_url",
    "NODE_ENV": "production"
  }
}

