{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "pnpm install --no-frozen-lockfile",
        "buildCommand": "pnpm prisma generate && pnpm build"
      }
    }
  ],
  "env": {
    "AWS_REGION": "@aws_region",
    "SECRET_NAME": "@secret_name",
    "DATABASE_URL": "@database_url"
  }
}
